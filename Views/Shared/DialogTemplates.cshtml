<div id="sendDialog" style="display: none" title="@Constants.i18n("send")">
    <div class="fleft inlineblock half emailChoiceContainer">
        <div class="fleft inlineblock fullWidth">
            <input type="radio" name="emailChoice" data-emailchoice="IsLink" checked="checked" />
            <span>@Constants.i18n("emailLink")*</span>
        </div>
        <div class="fleft inlineblock fullWidth">
            <input type="radio" name="emailChoice" data-emailchoice="IsAttachment" />
            <span>@Constants.i18n("emailAttachment")</span>
        </div>
        <div class="fleft inlineblock fullWidth">
            <input type="radio" name="emailChoice" data-emailchoice="IsLinkAttachment" />
            <span>@Constants.i18n("emailBoth")*</span>
        </div>
        <div class="fleft inlineblock fullWidth">
            <input type="radio" name="emailChoice" data-emailchoice="IsDirectLink" />
            <span>@Constants.i18n("emailDirectLink")*</span>
        </div>
    </div>
    <div class="fleft inlineblock half sendOptions">
        <div class="fleft inlineblock fullWidth">
            <span>@Constants.i18n("withPassword")</span>
            <input type="password" />
        </div>
        <div class="fleft inlineblock fullWidth pageSelectionContainer">
            <span>@Constants.i18n("selectPages")</span>
            <input type="text" class="pageSelection" />
            <div class="fleft inlineblock fullWidth indented">
                @Constants.i18n("pageSelectionExample")
            </div>
        </div>
        <div class="fleft inlineblock spacingRight">
            <input type="checkbox" name="Annotations" />
            <span>@Constants.i18n("withAnnotations")</span>
        </div>
        <div class="fleft inlineblock spacingRight">
            <input type="checkbox" name="Redactions" />
            <span>@Constants.i18n("withRedactions")</span>
        </div>
    </div>
    <div class="fleft inlineblock fullWidth exportType">
        <fieldset>
            <legend>@Constants.i18n("sendAs")</legend>
            @{
                var values = Enum.GetNames(typeof(ExportDocumentType));
                var isChecked = "checked=\"checked\"";
                foreach (var value in values)
                {
                    if (value == ExportDocumentType.Renderings.ToString() || value == ExportDocumentType.WebPrint.ToString())
                    {
                        continue;
                    }
                    <div class="inlineblock fleft">
                        <input type="radio" value="@value" name="ExportType" @isChecked />
                        <span>@value</span>
                    </div>
                    isChecked = "";
                }
            }
        </fieldset>
    </div>
    <div id="created-link" class="fleft fullWidth inlineblock">
        <div id="email_link" class="item link displayNone">
            <span class="created_link_label">@Constants.i18n("createdLink"):</span>
            <input id="copyLink" type="text" readonly="readonly" />
            <span class="generateLink sPng" title="@Constants.i18n("generateLink")"></span>
        </div>
        <div class="item emailProps">
            <span class="email_link_label">@Constants.i18n("address"):</span>
            <input id="email_address" type="text" />
        </div>
        <div class="item emailProps">
            <span id="subject_link_label">@Constants.i18n("subject"):</span>
            <input id="subject_link" type="text" />
        </div>
        <div class="item emailProps">
            <span class="email_body_label">@Constants.i18n("body"):</span>
            <textarea class="textareaResizeNone" id="email_body"></textarea>
        </div>
    </div>
    <div class="fleft" id="linkVersioningWarning" style="display: none">
        <span class="email_warning_label">*@Constants.i18n("emailLinksAreToCurrentOnly")</span>
    </div>
    <div class="fright">
        <input class="email_warning_label" type="checkbox" name="rememberSettings" checked="checked" />
        <span class="email_warning_label">@Constants.i18n("rememberSettings")</span>
    </div>
    <div id="downloadRedirectMessage" class="fullWidth inlineblock fleft spacingTopBottom">
        &nbsp;
    </div>
    <div class="fleft fullWidth inlineblock">
        <img id="sendThrobber" alt="@Constants.i18n("loadText")" src="@System.Web.Mvc.UrlHelper.GenerateContentUrl("~/Content/images/transparent.png", new HttpContextWrapper(HttpContext.Current))" />
        <input type="hidden" name="sendDialogError" />
        <span class="success_message" style="display: none;">@Constants.i18n("emailSuccess")</span>
    </div>
</div>
