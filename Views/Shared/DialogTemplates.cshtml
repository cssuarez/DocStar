        </li>
        <li>
            <label>@Constants.i18n("deskew"):</label>
            <input type="checkbox" name="Deskew" />
        </li>
        <li>
            <label>@Constants.i18n("despeckle"):</label>
            <input type="checkbox" name="Despeckle" />
        </li>
        <li>
            <label>@Constants.i18n("duplex"):</label>
            <input type="checkbox" name="Duplex" />
        </li>
        <li>
            <label>@Constants.i18n("color"):</label>
            <input type="radio" name="Color" value="@ColorSetting.BlackAndWhite.ToString()" checked="checked" />
            <span>@Constants.i18n("bw")</span>
            <input type="radio" name="Color" value="@ColorSetting.GreyScale.ToString()" />
            <span>@Constants.i18n("greyscale")</span>
            <input type="radio" name="Color" value="@ColorSetting.Color.ToString()" />
