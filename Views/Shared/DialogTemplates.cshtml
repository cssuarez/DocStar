        <li>
            <label>@Constants.i18n("device"):</label>
            <select name="deviceId"></select>
        </li>
        <li>
            <label>@Constants.i18n("resolution"):</label>
            <input type="text" name="Resolution" value="300" />
        </li>
        <li>
            <label>@Constants.i18n("pageSize"):</label>
            <select name="PageSize">
                @{
                    var pageSizes = Enum.GetValues(typeof(PageSize)).Cast<PageSize>().OrderBy(r => r).ToList();
                    pageSizes.Remove(PageSize.Letter);
                    pageSizes.Remove(PageSize.Legal);
                    pageSizes.Insert(0, PageSize.Legal);
                    pageSizes.Insert(0, PageSize.Letter);
                    foreach (var pageSize in pageSizes)
                    {
                        <option value="@((int)pageSize)">@pageSize.ToString()</option>
                    }
                }
            </select>
        </li>
        <li>
            <label>@Constants.i18n("removeBlanks"):</label>
            <input type="checkbox" name="RemoveBlankPages" />
        </li>
